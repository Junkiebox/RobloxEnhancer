{
   "key": "",
   "manifest_version": 2,
   "name": "Roblox Enhancer",
   "short_name": "Roblox Enhancer",
   "version": "0.81",
   "web_accessible_resources": [ "css/obc.css" ],

   "background": {
      "scripts": [ "scripts/jquery-ui.min.js","scripts/jquery.min.js", "scripts/MyPlugins.js", "scripts/background.js", "scripts/myscript.js", "scripts/popup.js"]
   },
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html",
      "default_title": "Click to update siggy!"
   },
   "content_scripts": [ {
      "css": [ "css/jquery.growl.css", "css/Anti-Stretch.css", "css/colpick.css", "css/video.css" ],
      "js": [ "scripts/jquery-ui.min.js","scripts/jquery.min.js", "scripts/obc.js", "scripts/applysig.js", "scripts/textchange.js","scripts/jquery.linkify.min.js", "scripts/jquery.growl.js", "scripts/colpick.js", "scripts/MyPlugins.js", "scripts/myscript.js","scripts/Embedding.js"],
      "matches": [ "*://*.roblox.com/*"],
      "run_at": "document_start"
   } ],
   "description": "Enhances webpages on Roblox!",
   "icons": {
      "128": "images/icon128.png",
      "48": "images/icon.png"
   },

      "permissions": [ "background", "notifications","storage", "*://*.roblox.com/*","*://assetgame.roblox.com/*","*://*.rbxcdn.com/*","*://github.com/*", "contextMenus", "https://archive.is/*" ]
}
